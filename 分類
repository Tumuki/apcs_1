#include <iostream>
using namespace std;
int main()
{
    int i;
    int sutd_num=45;
    for (i=0;i<10;i++){
        if((i % 3) == 0){
            printf("%d. hello world %d students \n",0, sutd_num);
        }else if((i % 3) == 1){
            printf("%d. hello world %d students \n",1, sutd_num);
        }else if((i % 3) == 2){  
            printf("%d. hello world %d students \n",2, sutd_num);
        }
    }
    return 0;
}
